/* описания для main.c */
 .text
      .global Start_DSP
      .global InA
      .global InI
      .global OutC


/* сегмент кода */
Start_DSP:MOVE (A0), R0.L
          MOVE (A1), R1.L

Do1:      SUBL R1.L,R0.L,R3.L
          FMPY R3.L,R0.L,R4.L
          J.ne Do1
          MOVE R4.L, (A2)
          STOP




/* сегмент данных */
 .data
InA:  .word 0
InI:  .word 0
OutC: .word 0
 .end
