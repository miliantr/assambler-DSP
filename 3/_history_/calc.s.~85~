.text
.global Start_DSP
.global Arr3

Start_DSP:
    MOVE 1, R0.L
    MOVE 1, R2.L
    MOVE Arr1, A1
    MOVE Arr2, A2
    MOVE Arr3, A3
    MOVE 4, I4
    MOVE 0, R8.L

    DO 32, L1
    MOVE R0.L, (A1)+
L1: NOP

    DO 32, L2
    MOVE R2.L, (A2)+
L2: NOP

    MOVE (Arr1), A1
    MOVE (Arr2), A2

    DO 4, L3
        DO 8, L4
            MOVE 0, R8.l
            MOVE A2.L, R0.L
            Move R0.L, A4.L
            DO 8, L5
                MOVE (A1)+, R2.L
                MOVE (A4)+I4, R4.L
                MPYl R2.L, R4.L, R6.D
                ADDl R6.l, R8.l
            L5: NOP
            MOVE R8.L, (A3)+
        L4: NOP
    L3: NOP
    STOP

.data
Arr1: .space 128, 0x00
Arr2: .space 128, 0x00
Arr3: .space 64, 0x00
.end

